{
  "name": "ai-assistant-vapi",
  "version": "1.0.0",
  "description": "Prototipo backend Fastify in TypeScript per assistente virtuale del Comune di Codroipo con integrazione Vapi e Google Calendar.",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "vercel-build": "npm run build",
    "start": "node dist/server.js",
    "scrape:services": "tsx scripts/scrape-services.ts",
    "parse:scraped": "tsx scripts/parse-scraped-data.ts",
    "generate:kb": "tsx scripts/generate-knowledge-base.ts",
    "sync:all": "npm run scrape:services && npm run parse:scraped && npm run generate:kb",
    "sync:vapi": "tsx scripts/sync-vapi.ts",
    "setup:vapi-tools": "tsx scripts/setup-vapi-tools.ts",
    "lint": "tsc --noEmit",
    "test": "echo \"No automated tests defined yet\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@vapi-ai/server-sdk": "^0.10.2",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "puppeteer": "^24.26.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@vercel/node": "^5.5.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
